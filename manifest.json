{
  "manifest_version": 3,
  "name": "AI Note Taker",
  "version": "1.0.0",
  "description": "Transform text into organized notes with AI-powered summarization and reflection",
  "permissions": [
    "storage",
    "aiLanguageModelOriginTrial",
    "aiSummarizerOriginTrial"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}